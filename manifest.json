{
  "manifest_version": 3,
  "name": "Auto Image blur",
  "version": "1.0",
  "description": "blur images on webpages automatically",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png"
  }
}
